<?xml version="1.0" encoding="utf-8"?>
<project>
	<meta title="CrossRoad" package="com.mt.dev.openfl" description="CrossRoad Game Demo" version="1.0.0" company="MotionTwin" />
	<app file="CrossRoad" main="App" path="bin" swf-version="11" />
	
	<window background="#000000" fps="60" />
	<window width="600" height="480" unless="mobile" />
	<window orientation="landscape" vsync="true" antialiasing="2"  />
	
	<source path="src" />
	
	<haxelib name="mt" />
	<haxelib name="openfl" />
	<haxelib name="most" />
	
	<haxedef  name="spriteParticles" />
	<!-- assets -->
	<icon name="assets/nme.svg" />    
	<assets path="assets" rename="" include="*" />
	
	<!-- libraries -->
	<library path="assets/gfx.swf" />
	
</project>
