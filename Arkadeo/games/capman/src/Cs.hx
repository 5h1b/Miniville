	public static function getTinyScore(sco:Int) {
		var mc = new SP();
		var a = Std.string(sco).split("");
		var px = -a.length * 2;
		for( char in a ) {
			var el = new EL();
			el.goto(char.charCodeAt(0) - 48, "num", 0, 0);
			el.x = px;
			mc.addChild(el);
			px += char == "1" ? 4 : 8;
		}
		return mc;
	}
	
	public static function getField(color=0xFFFFFF) {
		var f = new TF();
		var tf = f.getTextFormat();
		tf.color = color;
		tf.size = 8;
		tf.font = "nokia";
		f.embedFonts = true;
		f.defaultTextFormat = tf;
		f.selectable = false;
		f.width = 80;
		f.height = 14;
		return f;
	}
	
